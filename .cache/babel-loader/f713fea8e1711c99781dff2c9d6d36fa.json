<form class=\"mt-2\" @submit.prevent=\"onSubmit()\">\n              <div class=\"form-group\">\n                <label for=\"exampleInputEmail1\">الاسم</label>\n                <input type=\"text\" v-model=\"v$.form.name.$model\" class=\"form-control mb-0\" id=\"exampleInputEmail1\" placeholder=\"ادخال الاسم هنا\">\n                   <small style=\"color:red;\" v-if=\"v$.form.name.$error\">الرجاء قم بادخال اسمك كما هو في المستند الرسمي</small>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"exampleInputEmail2\">الايميل</label>\n                <input v-model=\"v$.form.email.$model\" type=\"email\" class=\"form-control mb-0\" id=\"exampleInputEmail2\" placeholder=\"ادخال الايميل هنا\">\n               <small style=\"color:red;\" v-if=\"v$.form.email.$error\">الرجاء قم بادخال بريدك الاكتروني</small>\n              </div>\n               <div class=\"form-group\">\n                <label for=\"exampleInputPassword1\">الرمز السري</label>\n                <input v-model=\"v$.form.password.$model\" type=\"password\" class=\"form-control mb-0\" id=\"exampleInputPassword1\" placeholder=\"ادخال ارمز السري هنا\">\n                <small style=\"color:red;\" v-if=\"v$.form.password.$error\">الرجاء قم بادخال كلمة السر</small>\n              </div>\n             \n              <div class=\"d-inline-block w-100\">\n                <button type=\"submit\" class=\"btn btn-primary float-end\" >انشاء حساب</button>\n              </div>\n              \n              <div class=\"sign-info\">\n                <span class=\"dark-color d-inline-block line-height-2\"> تملك حساب؟<router-link\n                  :to=\"{ name: 'auth1.sign-in1'}\"> تسجيل الدخول </router-link></span>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n</section>\n</template>\n<script>\nimport useVuelidate from '@vuelidate/core'\nimport { required, email, minLength } from '@vuelidate/validators'\n\nexport default