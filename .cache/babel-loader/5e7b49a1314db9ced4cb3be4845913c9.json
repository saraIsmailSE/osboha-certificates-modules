{"ast":null,"code":"import _regeneratorRuntime from \"C:/Users/someO/Desktop/vue/osboha-template/node_modules/@babel/runtime/helpers/esm/regeneratorRuntime.js\";\nimport _asyncToGenerator from \"C:/Users/someO/Desktop/vue/osboha-template/node_modules/@babel/runtime/helpers/esm/asyncToGenerator.js\";\nimport html2pdf from \"html2pdf.js\";\nimport certificateServices from \"../../api/certificateServices\";\nexport default {\n  name: \"Certificate\",\n  created: function created() {\n    var _this = this;\n\n    return _asyncToGenerator( /*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n      return _regeneratorRuntime().wrap(function _callee$(_context) {\n        while (1) {\n          switch (_context.prev = _context.next) {\n            case 0:\n              _context.next = 2;\n              return _this.getData();\n\n            case 2:\n            case \"end\":\n              return _context.stop();\n          }\n        }\n      }, _callee);\n    }))();\n  },\n  data: function data() {\n    return {\n      certificate: {\n        user: {},\n        book: {},\n        certificates: [{}],\n        general_information: {},\n        thesises: {},\n        questions: {}\n      }\n    };\n  },\n  methods: {\n    getData: function getData() {\n      var _this2 = this;\n\n      return _asyncToGenerator( /*#__PURE__*/_regeneratorRuntime().mark(function _callee2() {\n        var response;\n        return _regeneratorRuntime().wrap(function _callee2$(_context2) {\n          while (1) {\n            switch (_context2.prev = _context2.next) {\n              case 0:\n                _context2.next = 2;\n                return certificateServices.fullCretificate(_this2.$route.params.id);\n\n              case 2:\n                response = _context2.sent;\n                _this2.certificate = response[0];\n\n              case 4:\n              case \"end\":\n                return _context2.stop();\n            }\n          }\n        }, _callee2);\n      }))();\n    },\n    getDegree: function getDegree(degree) {\n      var textDegree = \"\";\n      if (degree <= 100 && degree > 94) textDegree = \"أمتياز\";else if (degree < 95 && degree > 89.9) textDegree = \"ممتاز\";else if (degree < 90 && degree > 84.9) textDegree = \"جيد جدا\";else if (degree < 85 && degree > 79.9) textDegree = \"جيد\";else if (degree > 69.9 && degree < 80) textDegree = \"مقبول\";\n      return textDegree;\n    },\n    exportToPDF: function exportToPDF() {\n      html2pdf(document.getElementById(\"mainpage\"), {\n        // eslint-disable-next-line\n        image: {\n          type: \"jpg\",\n          quality: 20\n        },\n        html2canvas: {\n          scale: 2\n        },\n        filename: \"i-was-html.pdf\"\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;AAsPA,OAAOA,QAAP,MAAqB,aAArB;AACA,OAAOC,mBAAP,MAAgC,+BAAhC;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEPC,OAFO,qBAEG;IAAA;;IAAA;MAAA;QAAA;UAAA;YAAA;cAAA;cAAA,OACR,KAAI,CAACC,OAAL,EADQ;;YAAA;YAAA;cAAA;UAAA;QAAA;MAAA;IAAA;EAEf,CAJY;EAKbC,IALa,kBAKN;IACL,OAAO;MACLC,WAAW,EAAE;QACXC,IAAI,EAAE,EADK;QAEXC,IAAI,EAAE,EAFK;QAGXC,YAAY,EAAE,CAAC,EAAD,CAHH;QAIXC,mBAAmB,EAAE,EAJV;QAKXC,QAAQ,EAAE,EALC;QAMXC,SAAS,EAAE;MANA;IADR,CAAP;EAUD,CAhBY;EAiBbC,OAAO,EAAE;IACDT,OADC,qBACS;MAAA;;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OACSH,mBAAmB,CAACa,eAApB,CACrB,MAAI,CAACC,MAAL,CAAYC,MAAZ,CAAmBC,EADE,CADT;;cAAA;gBACRC,QADQ;gBAId,MAAI,CAACZ,WAAL,GAAmBY,QAAQ,CAAC,CAAD,CAA3B;;cAJc;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAKf,CANM;IAOPC,SAPO,qBAOGC,MAPH,EAOW;MAChB,IAAIC,UAAS,GAAI,EAAjB;MAEA,IAAID,MAAK,IAAK,GAAV,IAAiBA,MAAK,GAAI,EAA9B,EAAkCC,UAAS,GAAI,QAAb,CAAlC,KACK,IAAID,MAAK,GAAI,EAAT,IAAeA,MAAK,GAAI,IAA5B,EAAkCC,UAAS,GAAI,OAAb,CAAlC,KACA,IAAID,MAAK,GAAI,EAAT,IAAeA,MAAK,GAAI,IAA5B,EAAkCC,UAAS,GAAI,SAAb,CAAlC,KACA,IAAID,MAAK,GAAI,EAAT,IAAeA,MAAK,GAAI,IAA5B,EAAkCC,UAAS,GAAI,KAAb,CAAlC,KACA,IAAID,MAAK,GAAI,IAAT,IAAiBA,MAAK,GAAK,EAA/B,EAAqCC,UAAS,GAAI,OAAb;MAE1C,OAAOA,UAAP;IACD,CAjBM;IAkBPC,WAlBO,yBAkBO;MACZtB,QAAQ,CAACuB,QAAQ,CAACC,cAAT,CAAwB,UAAxB,CAAD,EAAsC;QAC5C;QACAC,KAAK,EAAE;UAAEC,IAAI,EAAE,KAAR;UAAeC,OAAO,EAAE;QAAxB,CAFqC;QAG5CC,WAAW,EAAE;UAAEC,KAAK,EAAE;QAAT,CAH+B;QAI5CC,QAAQ,EAAE;MAJkC,CAAtC,CAAR;IAMD;EAzBM;AAjBI,CAAf","names":["html2pdf","certificateServices","name","created","getData","data","certificate","user","book","certificates","general_information","thesises","questions","methods","fullCretificate","$route","params","id","response","getDegree","degree","textDegree","exportToPDF","document","getElementById","image","type","quality","html2canvas","scale","filename"],"sourceRoot":"","sources":["C:\\Users\\someO\\Desktop\\vue\\osboha-template\\src\\views\\Certificate\\Certificate.vue"],"sourcesContent":["<template>\n  <div class=\"w-50 text-center\">\n    <div id=\"export\">\n      <button\n        class=\"get-certificate-btn btn btn-primary mb-2 w-25\"\n        @click=\"exportToPDF\"\n      >\n        تحميل الشهادة ك ملف pdf\n      </button>\n    </div>\n    <div id=\"mainpage\">\n      <div class=\"page\">\n        <div class=\"page-content\">\n          <p style=\"text-align: center\">\n            <span style=\"font-size: 30pt; font-family: 'Arial', sans-serif\"\n              >بسم الله الرحمن الرحيم\n            </span>\n          </p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p>التاريخ:</p>\n\n          <p class=\"main-header\" style=\"text-align: center\">\n            <u><span>توثيق إنجاز قارئ</span></u>\n          </p>\n          <p style=\"text-align: center\">\n            <strong>مستوى الكتاب: (بسيط/متوسط/عميق)</strong>\n          </p>\n          <p>\n            <strong><span style=\"font-size: 13px\"> </span></strong>\n          </p>\n          <p>\n            تثبت هذه الوثيقة من مشروع صناعة القرّاء (أصبوحة 180) بأن الحاصل\n            عليها بموجب المعايير المتبعة يحمل الاسم\n            <span class=\"important-text\"> {{ certificate.user.name }} </span>،\n            قد طبق معايير القراءة المنهجية من خلال إنجازه قراءة كتاب\n            {{ certificate.book.book_name }}. ويثمن المشروع جهوده المبذولة وسعيه\n            الحثيث في زيادة حصيلته المعرفية، ورغبته باستمرارية التعلم عبر\n            القراءة المنهجية؛ ليبني صرحه المعرفيّ الذي يؤسّس لما بعده من\n            الأفعال، والتي تؤدي بدورها إلى النّهضة الحقيقيّة على المستوى الفردي\n            والجماعي، فينفع أمّته ويساهم في بناء نهضتها نحو القوّة والتطور\n            والرفعة.\n          </p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p>\n            علمًا أن مشروع أصبوحة 180 هو الحاضنة الأساسية للقرّاء، بصفته المشروع\n            الأكبر عربيّاً لصناعة الفكر، مُركزًا على عوامل الفكر عن طريق استثمار\n            القراءة المنهجيّة ونواتجها؛ لصناعة مجتمع واعٍ قادرٍ على الوصول\n            للنّهضة، وتحقيق التنمية مستعيناً بالتكنولوجيا الحديثة.\n          </p>\n        </div>\n      </div>\n      <div class=\"page\">\n        <div class=\"page-content\">\n          <p class=\"sub-header\">التقييم الفردي لحامل هذه الشهادة</p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p>\n            <span>يهدف مشروع أصبوحة </span><span>180 </span\n            ><span>\n              إلى رفع كفاءة التعلم من خلال القراءة المنهجية لدى القارئ وذلك من\n              خلال تعميق أهمية الكتابة التوثيقية المصاحبة للقراءة و المتمثلة في\n              تنظيم وحفظ أوعية المعرفة، وتيسير سبل الإفادة من محتوياتها؛ لأغراض\n              المناقشة والمقارنة، بهدف تحقيق أقصى درجات الإفادة منها، و</span\n            ><span\n              >استثمار أكبر قدر ممكن من النتاج الفكري لتتحول القراءة عند القارئ\n              من مجرد اكتساب إلى عملية تبادلية فيها الاكتساب والنتاج المتولد\n              عنها</span\n            ><span\n              >، إيمانًا منا أن إعمال الذهن فيما تتعلم، وتوثيقه كتابيًا يعد\n              وسيلة أساسية للابتكار والتطور وتحقيق النهضة.</span\n            >\n          </p>\n          <p>\n            <span\n              >لقد قام فريق مختص بمتابعة القارئ و قام فريق مدرب على تقييم إنجازه\n              ضمن خطة منهجية ومعايير محددة مسبقًا، اعتمدت على قراءة الأفكار\n              الأساسية والفرعية الموجودة في الكتاب، وتقديم تغذية راجعة للأطروحات\n              المقدّمة للمساهمة في رفع استفادة الفرد من حصيلة ما قد قرأ وتعزيز\n              مهارات القراءة الفعّالة من خلال مناقشات وحوارات، وتمكينه من ترسيخ\n              أفكار المادة المقروءة وتحويلها إلى أسئلة معرفية، وكلمات دلالية،\n              وملخصات مرجعية، وأطروحات ناتجة عن حصاد ذلك كله.</span\n            >\n          </p>\n          <p>\n            <strong\n              ><span\n                ><br />\n                <u>التقديرات التي حصل عليها القارئ كما يلي: </u></span\n              ></strong\n            >\n          </p>\n\n          <p>\n            - السؤال العام المقدم من القارئ تقديره ( \n      <span v-bind:class = \"(certificate.certificates[0].general_summary_grade <= 100 && certificate.certificates[0].general_summary_grade > 94) ? 'highlight' : '' \">أمتياز</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n           <span  v-bind:class = \"(certificate.certificates[0].general_summary_grade <= 95 && certificate.certificates[0].general_summary_grade > 89.9) ? 'highlight' : '' \">ممتاز</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].general_summary_grade <= 90 && certificate.certificates[0].general_summary_grade > 84.9) ? 'highlight' : '' \">جيد جدا</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].general_summary_grade <= 80 && certificate.certificates[0].general_summary_grade > 79.9) ? 'highlight' : '' \">جيد</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].general_summary_grade <= 70 && certificate.certificates[0].general_summary_grade > 69.9) ? 'highlight' : '' \">مقبول)</span>\n          </p>\n          <p>\n            - الأسئلة المعرفية المقدمة من القارئ تقديرها ( \n              <span v-bind:class = \"(certificate.certificates[0].check_reading_grade <= 100 && certificate.certificates[0].check_reading_grade > 94) ? 'highlight' : '' \">أمتياز</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n           <span  v-bind:class = \"(certificate.certificates[0].check_reading_grade <= 95 && certificate.certificates[0].check_reading_grade > 89.9) ? 'highlight' : '' \">ممتاز</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].check_reading_grade <= 90 && certificate.certificates[0].check_reading_grade > 84.9) ? 'highlight' : '' \">جيد جدا</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].check_reading_grade <= 80 && certificate.certificates[0].check_reading_grade > 79.9) ? 'highlight' : '' \">جيد</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].check_reading_grade <= 70 && certificate.certificates[0].check_reading_grade > 69.9) ? 'highlight' : '' \">مقبول)</span>\n          \n          </p>\n          <p>\n            - الأطروحات المقدمة من القارئ تقديرها ( \n              <span v-bind:class = \"(certificate.certificates[0].thesis_grade <= 100 && certificate.certificates[0].thesis_grade > 94) ? 'highlight' : '' \">أمتياز</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n           <span  v-bind:class = \"(certificate.certificates[0].thesis_grade <= 95 && certificate.certificates[0].thesis_grade > 89.9) ? 'highlight' : '' \">ممتاز</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].thesis_grade <= 90 && certificate.certificates[0].thesis_grade > 84.9) ? 'highlight' : '' \">جيد جدا</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].thesis_grade <= 80 && certificate.certificates[0].thesis_grade > 79.9) ? 'highlight' : '' \">جيد</span>\n            <strong><span style=\"color: #00b050\">||</span></strong>\n          <span  v-bind:class = \"(certificate.certificates[0].thesis_grade <= 70 && certificate.certificates[0].thesis_grade > 69.9) ? 'highlight' : '' \">مقبول)</span>\n          </p>\n        </div>\n      </div>\n      <div class=\"page\">\n        <div class=\"page-content\">\n          <p class=\"sub-header\">شرح آلية تقدم القارئ للحصول على التوثيق</p>\n          <p class=\"break-space\">&nbsp;</p>\n\n          <p>\n            يقوم القارئ باختيار كتاب منهجي من مكتبة أصبوحة وقراءته، ثمّ يطلب منه\n            كتابة من8 أطروحات كحدّ أدنى إلى 12 كحدّ أقصى، تخضع هذه الأطروحات\n            للتدقيق من قبل الفريق المختص، ثمّ تأتي مرحلة تأكيد القراءة الفاحصة\n            من خلال تلخيص الفكرة العامة للكتاب، وطرح سؤال عام لمحتواه ثم\n            تحديد(5- 12) سؤالا شاملاً لمضامين الكتاب مع ذكر أرقام الصفحات التي\n            تعالجها هذه الأسئلة، وإرفاق الإجابات النموذجية لها كاقتباسات صحيحة\n            للأسئلة. ويتم قياس نتاج الحصيلة المعرفية الناتجة عن المادة المقروءة\n            من الكتاب من خلال اختبار مستوى الأطروحات الناتجة عن الأسئلة المعرفية\n            والتلاخيص المستفادة.\n          </p>\n          <p style=\"color: #00b050\">\n            تنشر كافة ملفات التوثيق في الموقع الرسمي لمشروع صناعة القراء\n            (أصبوحة180) وذلك لإتاحة التحقق من مصداقية منح المشروع للوثائق\n            لحامليها.\n          </p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"sub-header\">\n            نرفق لكم في الصفحات التالية نموذج الطلب المقدمن من القارئ والتي\n            رشحته للحصول على هذا الاستحقاق وهذه التقديرات.\n          </p>\n        </div>\n      </div>\n\n      <!-- ##### GENERAL INFORMATION ##### -->\n      <div class=\"page\">\n        <div class=\"page-content\">\n          <p class=\"main-header\" style=\"text-align: center\">\n            <u><span>التلخيص العام</span></u>\n          </p>\n          <p class=\"mb-0 text-primary\">\n            التقييم ||\n            {{ getDegree(certificate.certificates[0].general_summary_grade) }}\n          </p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"sub-header\">التلخيص العام</p>\n\n          <p>\n            {{ certificate.general_information.general_question }}\n          </p>\n          <p class=\"sub-header\">السؤال العام</p>\n          <p>\n            {{ certificate.general_information.summary }}\n          </p>\n        </div>\n      </div>\n      <!-- ##### END GRNRRAL INFORMATION ##### -->\n\n      <!-- ##### THESES ##### -->\n      <div\n        class=\"page\"\n        v-for=\"(thesis, index) in certificate.thesises\"\n        :key=\"index\"\n      >\n        <div class=\"page-content\">\n          <p v-if=\"index === 0\" class=\"main-header\" style=\"text-align: center\">\n            <u><span>الأطروحات</span></u>\n          </p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"break-space\">&nbsp;</p>\n\n          <p class=\"sub-header\">أطروحة {{ index + 1 }}</p>\n          <p class=\"mb-0 text-primary\">\n            التقييم || {{ getDegree(thesis.degree) }}\n          </p>\n          <p>\n            {{ thesis.thesis_text }}\n          </p>\n        </div>\n      </div>\n\n      <!-- ##### END THESES ##### -->\n\n      <!-- ##### QUESTIONS ##### -->\n      <div\n        class=\"page\"\n        v-for=\"(question, index) in certificate.questions\"\n        :key=\"index\"\n      >\n        <div class=\"page-content\">\n          <p class=\"main-header\" style=\"text-align: center\">\n            <u><span>الأسئلة المعرفية</span></u>\n          </p>\n          <p class=\"break-space\">&nbsp;</p>\n          <p class=\"sub-header\">سؤال {{ index + 1 }}</p>\n          <p class=\"mb-0 text-primary\">\n            التقييم || {{ getDegree(question.degree) }}\n          </p>\n          <p>\n            {{ question.question }}\n          </p>\n          <p class=\"sub-header\">الاقتباسات</p>\n          <ul>\n            <li v-for=\"(quote, index) in question.quotation\" :key=\"index\">\n              <p class=\"quote\" dir=\"rtl\">\n                {{ quote.text }}\n              </p>\n            </li>\n          </ul>\n        </div>\n      </div>\n\n      <!-- ##### END QUESTIONS ##### -->\n    </div>\n  </div>\n</template>\n<script>\nimport html2pdf from \"html2pdf.js\";\nimport certificateServices from \"../../api/certificateServices\";\n\nexport default {\n  name: \"Certificate\",\n  async created() {\n    await this.getData();\n  },\n  data() {\n    return {\n      certificate: {\n        user: {},\n        book: {},\n        certificates: [{}],\n        general_information: {},\n        thesises: {},\n        questions: {},\n      },\n    };\n  },\n  methods: {\n    async getData() {\n      const response = await certificateServices.fullCretificate(\n        this.$route.params.id\n      );\n      this.certificate = response[0];\n    },\n    getDegree(degree) {\n      let textDegree = \"\";\n\n      if (degree <= 100 && degree > 94) textDegree = \"أمتياز\";\n      else if (degree < 95 && degree > 89.9) textDegree = \"ممتاز\";\n      else if (degree < 90 && degree > 84.9) textDegree = \"جيد جدا\";\n      else if (degree < 85 && degree > 79.9) textDegree = \"جيد\";\n      else if (degree > 69.9 && degree <  80  ) textDegree = \"مقبول\";\n\n      return textDegree;\n    },\n    exportToPDF() {\n      html2pdf(document.getElementById(\"mainpage\"), {\n        // eslint-disable-next-line\n        image: { type: \"jpg\", quality: 20 },\n        html2canvas: { scale: 2 },\n        filename: \"i-was-html.pdf\",\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.get-certificate-btn {\n  position: fixed;\n  font-size: 20px;\n}\n\n.page-content {\n  margin-top: 30%;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  width: 80%;\n}\n\n.page {\n  font-family: \"Calibri, sans-serif\";\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n  width: 210mm;\n  height: 297mm;\n  background-image: url(\"../../assets/images/certMain.jpg\");\n  background-size: cover;\n  background-position: left;\n  margin: auto auto;\n  color: black;\n}\n\n.page p {\n  font-size: 14pt;\n  margin-left: 1px;\n  margin-bottom: 10pt;\n  line-height: 150%;\n  direction: rtl;\n  text-align: right;\n}\n\n.page .main-header {\n  line-height: 115%;\n  font-weight: bold;\n  font-size: 36pt;\n}\n\n.page .sub-header {\n  line-height: 115%;\n  font-size: 18pt;\n  font-weight: bold;\n}\n\n.page .break-space {\n  line-height: 115%;\n}\n\n.important-text {\n  font-weight: bold;\n}\n\n.highlight {\n  background-color: yellow;\n  color: black;\n}\n\n.quote {\n  font-size: 10pt !important;\n}\n\n#signature {\n  width: 250px;\n  position: relative;\n  top: 63px;\n  left: -182px;\n  bottom: 0;\n  margin-top: -50px;\n  margin-left: -50px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}