{"ast":null,"code":"// Vue / Vuex Functions\nimport { ref, onMounted, onUnmounted, computed, watch } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router'; // Style Components\n\nimport ThemeScheme from './sections/ThemeScheme.vue';\nimport ColorCustomizer from './sections/ColorCustomizer.vue';\nimport MenuStyle from './sections/MenuStyle.vue';\nimport MenuActiveStyle from './sections/MenuActiveStyle.vue';\nimport Direction from './sections/Direction.vue';\nexport default {\n  name: 'SettingOffcanvas',\n  components: {\n    // Style Components\n    ThemeScheme: ThemeScheme,\n    ColorCustomizer: ColorCustomizer,\n    MenuStyle: MenuStyle,\n    MenuActiveStyle: MenuActiveStyle,\n    Direction: Direction\n  },\n  setup: function setup() {\n    // Store objects\n    var store = useStore();\n    var route = useRoute();\n    var themeSchemeDirection = computed(function () {\n      return store.getters['setting/theme_scheme_direction'];\n    }); // Variables\n\n    var show = ref(false); // Functions\n\n    var close = function close() {\n      show.value = false;\n    };\n\n    var open = function open() {\n      show.value = true;\n    };\n\n    var onClick = function onClick(e) {\n      if (show.value) {\n        if (e.target.closest('.live-customizer') == null && e.target.closest('#settingbutton') == null) {\n          show.value = false;\n        }\n      }\n    };\n\n    var defaultSetting = function defaultSetting() {\n      store.dispatch('setting/reset_state');\n    };\n\n    var isShowSetting = function isShowSetting(name) {\n      return !route.meta[name];\n    }; // Life cycle hooks\n\n\n    watch(themeSchemeDirection, function () {\n      show.value = false;\n    });\n    onMounted(function () {\n      document.body.addEventListener('click', onClick);\n    });\n    onUnmounted(function () {\n      document.body.removeEventListener('click', onClick);\n    }); // Return functions & variables\n\n    return {\n      themeSchemeDirection: themeSchemeDirection,\n      show: show,\n      isShowSetting: isShowSetting,\n      defaultSetting: defaultSetting,\n      close: close,\n      open: open\n    };\n  }\n};","map":{"version":3,"mappings":"AAwCA;AACA,SAASA,GAAT,EAAcC,SAAd,EAAyBC,WAAzB,EAAsCC,QAAtC,EAAgDC,KAAhD,QAA6D,KAA7D;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB,C,CAEA;;AACA,OAAOC,WAAP,MAAwB,4BAAxB;AACA,OAAOC,eAAP,MAA4B,gCAA5B;AACA,OAAOC,SAAP,MAAsB,0BAAtB;AACA,OAAOC,eAAP,MAA4B,gCAA5B;AACA,OAAOC,SAAP,MAAsB,0BAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,kBADO;EAEbC,UAAU,EAAE;IACV;IACAN,WAAW,EAAXA,WAFU;IAGVC,eAAe,EAAfA,eAHU;IAIVC,SAAS,EAATA,SAJU;IAKVC,eAAe,EAAfA,eALU;IAMVC,SAAQ,EAARA;EANU,CAFC;EAUbG,KAVa,mBAUJ;IACP;IACA,IAAMC,KAAI,GAAIV,QAAQ,EAAtB;IACA,IAAMW,KAAI,GAAIV,QAAQ,EAAtB;IACA,IAAMW,oBAAmB,GAAId,QAAQ,CAAC;MAAA,OAAMY,KAAK,CAACG,OAAN,CAAc,gCAAd,CAAN;IAAA,CAAD,CAArC,CAJO,CAMP;;IACA,IAAMC,IAAG,GAAInB,GAAG,CAAC,KAAD,CAAhB,CAPO,CASP;;IACA,IAAMoB,KAAI,GAAI,SAARA,KAAQ,GAAM;MAClBD,IAAI,CAACE,KAAL,GAAa,KAAb;IACF,CAFA;;IAGA,IAAMC,IAAG,GAAI,SAAPA,IAAO,GAAM;MACjBH,IAAI,CAACE,KAAL,GAAa,IAAb;IACF,CAFA;;IAGA,IAAME,OAAM,GAAI,SAAVA,OAAU,CAACC,CAAD,EAAO;MACrB,IAAIL,IAAI,CAACE,KAAT,EAAgB;QACd,IAAIG,CAAC,CAACC,MAAF,CAASC,OAAT,CAAiB,kBAAjB,KAAwC,IAAxC,IAAgDF,CAAC,CAACC,MAAF,CAASC,OAAT,CAAiB,gBAAjB,KAAsC,IAA1F,EAAgG;UAC9FP,IAAI,CAACE,KAAL,GAAa,KAAb;QACF;MACF;IACF,CANA;;IAOA,IAAMM,cAAa,GAAI,SAAjBA,cAAiB,GAAM;MAC3BZ,KAAK,CAACa,QAAN,CAAe,qBAAf;IACF,CAFA;;IAGA,IAAMC,aAAY,GAAI,SAAhBA,aAAgB,CAACjB,IAAD,EAAU;MAC9B,OAAO,CAACI,KAAK,CAACc,IAAN,CAAWlB,IAAX,CAAR;IACF,CAFA,CA1BO,CA8BP;;;IACAR,KAAK,CAACa,oBAAD,EAAuB,YAAM;MAChCE,IAAI,CAACE,KAAL,GAAa,KAAb;IACD,CAFI,CAAL;IAGApB,SAAS,CAAC,YAAM;MACd8B,QAAQ,CAACC,IAAT,CAAcC,gBAAd,CAA+B,OAA/B,EAAwCV,OAAxC;IACD,CAFQ,CAAT;IAGArB,WAAW,CAAC,YAAM;MAChB6B,QAAQ,CAACC,IAAT,CAAcE,mBAAd,CAAkC,OAAlC,EAA2CX,OAA3C;IACD,CAFU,CAAX,CArCO,CAyCP;;IACA,OAAO;MACLN,oBAAoB,EAApBA,oBADK;MAELE,IAAI,EAAJA,IAFK;MAGLU,aAAa,EAAbA,aAHK;MAILF,cAAc,EAAdA,cAJK;MAKLP,KAAK,EAALA,KALK;MAMLE,IAAG,EAAHA;IANK,CAAP;EAQF;AA5Da,CAAf","names":["ref","onMounted","onUnmounted","computed","watch","useStore","useRoute","ThemeScheme","ColorCustomizer","MenuStyle","MenuActiveStyle","Direction","name","components","setup","store","route","themeSchemeDirection","getters","show","close","value","open","onClick","e","target","closest","defaultSetting","dispatch","isShowSetting","meta","document","body","addEventListener","removeEventListener"],"sourceRoot":"","sources":["C:\\Users\\someO\\Desktop\\vue\\osboha-template\\src\\components\\custom\\setting\\SettingOffcanvas.vue"],"sourcesContent":["<template>\n  <!-- Setting offcanvas start here -->\n  <b-offcanvas v-model=\"show\" :backdrop=\"false\" title=\"\" :placement=\"themeSchemeDirection !== 'ltr' ? 'start' : 'end'\" class=\"live-customizer\" id=\"live-customizer\" :body-scrolling=\"true\">\n    <template #title>\n      <div class=\"d-flex align-items-center justify-content-between\">\n        Setting Pannel\n      </div>\n    </template>\n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n        <div>\n          <div class=\"text-center\">\n            <h5 class=\"d-inline-block\">Style Setting</h5>\n          </div>\n          <div>\n            <!-- Theme start here -->\n              <theme-scheme />\n            <!-- Theme end here -->\n            <hr/>\n            <!-- Menu Style start here -->\n            <menu-style v-if=\"isShowSetting('isSidebar')\" />\n            <!-- Menu Style end here -->\n            <!-- Active Menu Style start here -->\n            <menu-active-style v-if=\"isShowSetting('isSidebar')\" />\n            <!-- Active Menu Style end here -->\n            <hr/>\n            <!-- Color customizer start here -->\n            <color-customizer />\n            <!-- Color customizer end here -->\n            <hr/>\n            <direction />\n          </div>\n\n        </div>\n      </div>\n    </div>\n  </b-offcanvas>\n  <!-- Settings sidebar end here -->\n</template>\n<script>\n// Vue / Vuex Functions\nimport { ref, onMounted, onUnmounted, computed, watch } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute } from 'vue-router'\n\n// Style Components\nimport ThemeScheme from './sections/ThemeScheme.vue'\nimport ColorCustomizer from './sections/ColorCustomizer.vue'\nimport MenuStyle from './sections/MenuStyle.vue'\nimport MenuActiveStyle from './sections/MenuActiveStyle.vue'\nimport Direction from './sections/Direction.vue'\n\nexport default {\n  name: 'SettingOffcanvas',\n  components: {\n    // Style Components\n    ThemeScheme,\n    ColorCustomizer,\n    MenuStyle,\n    MenuActiveStyle,\n    Direction\n  },\n  setup () {\n    // Store objects\n    const store = useStore()\n    const route = useRoute()\n    const themeSchemeDirection = computed(() => store.getters['setting/theme_scheme_direction'])\n\n    // Variables\n    const show = ref(false)\n\n    // Functions\n    const close = () => {\n      show.value = false\n    }\n    const open = () => {\n      show.value = true\n    }\n    const onClick = (e) => {\n      if (show.value) {\n        if (e.target.closest('.live-customizer') == null && e.target.closest('#settingbutton') == null) {\n          show.value = false\n        }\n      }\n    }\n    const defaultSetting = () => {\n      store.dispatch('setting/reset_state')\n    }\n    const isShowSetting = (name) => {\n      return !route.meta[name]\n    }\n\n    // Life cycle hooks\n    watch(themeSchemeDirection, () => {\n      show.value = false\n    })\n    onMounted(() => {\n      document.body.addEventListener('click', onClick)\n    })\n    onUnmounted(() => {\n      document.body.removeEventListener('click', onClick)\n    })\n\n    // Return functions & variables\n    return {\n      themeSchemeDirection,\n      show,\n      isShowSetting,\n      defaultSetting,\n      close,\n      open\n    }\n  }\n}\n</script>\n<style>\n.live-customizer .offcanvas-title {\n  font-size: calc(1.269rem + 0.228vw);\n  flex: 1;\n}\n@media (min-width: 1200px) {\n  .live-customizer .offcanvas-title {\n    font-size: 1.44rem;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}