-3\">\n                        <div class=\"post-text ml-3 w-100 row\">\n              <div class=\"row\">\n\n                <div class=\"col-md-12 form-group\">\n                  <div class=\"col-md-12 form-group\">\n                    <h4 class=\"form-label\"> التلخيص العام</h4>\n                    <p class=\"form-control-plaintext\">{{ general_informations.summary }}</p>\n                  </div>\n\n                </div>\n                <div class=\"col-md-12 form-group\">\n                  <div class=\"col-md-12 form-group\">\n                    <h4 class=\"form-label\"> السؤال العام </h4>\n                    <p class=\"form-control-plaintext\">{{ general_informations.general_question }}</p>\n                  </div>\n\n                </div>\n              </div>\n\n            </div>\n\n          </div>\n\n          <div v-else class=\"d-flex align-items-center mt-3\">\n            <form @submit.prevent=\"submitGenralInformationsForm\" class=\"post-text ml-3 w-100 row\">\n              <div class=\"form-group\">\n                <label class=\"form-label\" for=\"bookGeneralIdea\">الفكرة العامة</label>\n                <textarea v-model=\"v$.generalInfromationsform.summary.$model\" rows=\"5\" placeholder=\"... اكتب فكرة عامة\"\n                  class=\"rounded form-control\" id=\"bookGeneralIdea\"></textarea>\n              </div>\n              <div class=\"form-group\">\n                <label class=\"form-label\" for=\"bookGeneralQuestion\">سؤال عام </label>\n                <textarea v-model=\"v$.generalInfromationsform.general_question.$model\" rows=\"5\"\n                  placeholder=\"... اكتب سؤالًا عاماً\" class=\"rounded form-control\" id=\"bookGeneralQuestion\"></textarea>\n              </div>\n\n              <input type=\"submit\" value=\"أضافة\" href=\"javascript:void(0);\" class=\"btn btn-primary d-block mt-3\" />\n            </form>\n\n          </div>\n\n\n        </tab-content-item>\n        <!-- END GENERAL IDEA -->\n\n        <!-- START QUESTIONS -->\n        <tab-content-item :active=\"false\" id=\"questions\" aria-labelled-by=\"questions\">\n          <h4>إضافة أسئلة معرفية حول الكتاب</h4>\n          <hr>\n          <div class=\"row\">\n            <div class=\"col-12\">\n              <h6>لوريم ايبسوم دولار سيت أميت ,كونسيكتيتور أدايبا يسكينج أليايت,سيت دو أيوسمود تيمبور\n\n                أنكايديديونتيوت لابوري ات دولار ماجنا أليكيوا . يوت انيم أد مينيم فينايم,كيواس نوستريد\n\n                أكسير سيتاشن يللأمكو لابورأس نيسي يت أليكيوب أكس أيا كوممودو كونسيكيوات . </h6>\n            </div>\n          </div>\n          <div v-if=\"(!( questions ) || questions.length < 12)\" class=\"d-flex align-items-center mt-3\">\n            <form @submit.prevent=\"submitQuestionForm\" class=\"post-text ml-3 w-100 row\">\n              <div class=\"form-group col-6\">\n                <select class=\"form-select\" data-trigger name=\"choices-single-default\" id=\"choices-single-default\" v-model=\"v$.questionForm.pageStart.$model\">\n                  <option value=\"\">اختر صفحة البداية</option>\n                  <option v-for=\"(i, index) in book.pages\" :key=\"index\" :value=\"i\">\n                  {{ i }}\n                </option>\n                </select>\n              </div>\n              <div class=\"form-group col-6\">\n\n                <select class=\"form-select\" data-trigger name=\"choices-single-default\" id=\"choices-single-default\" v-model=\"v$.questionForm.pageEnd.$model\">\n                  <option value=\"\">اختر صفحة النهاية</option>\n                  <option v-for=\"(i, index) in book.pages\" :key=\"index\" :value=\"i\">\n                  {{ i }}\n                </option>\n                </select>\n              </div>\n\n              <div class=\"form-group\">\n                <label class=\"form-label\" for=\"bookGeneralQuestion\">اضافة سؤال </label>\n                <textarea rows=\"5\" placeholder=\"... اكتب سؤالًا \" class=\"rounded form-control\" id=\"bookQuestion\"\n                  v-model=\"v$.questionForm.question.$model\"></textarea>\n              </div>\n\n              <div class=\"form-group\">\n                <a class=\"btn btn-primary rounded-pill mb-3 me-1\" @click=\"addField(textarea, quotes)\">\n                  اضافة اقتباس\n                </a>\n                <a class=\"btn btn-danger rounded-pill mb-3 me-1\" v-show=\"quotes.length > 1\"\n                  @click=\"removeField(index, quotes)\">\n                  حذف اقتباس\n                </a>\n                <textarea rows=\"5\" placeholder=\"... اكتب اقتباسًا \" v-for=\"qoute in quotes\" :key=\"qoute.id\"\n                  class=\"rounded form-control mt-2\" id=\"bookQuote\" v-model=\"qoute.qoute\">\n                </textarea>\n\n              </div>\n            </form>\n          </div>\n          <hr />\n          <a href=\"javascript:void(0);\" @click=\"submitQuestionForm\" class=\"btn btn-primary d-block mt-3\" >إضافة</a>\n          <div v-for=\"question in questions\" :key=\"question.id\">\n            <ListQuestion :question=\"question\" />\n          </div>\n\n        </tab-content-item>\n        <!-- END QUESTIONS -->\n\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport ListQuestion from '../../../../components/Questions/ListQuestion.vue'\nimport useVuelidate from \"@vuelidate/core\";\nimport { required, integer, requiredIf } from \"@vuelidate/validators\";\nimport { addQuestion } from '../../../../api/questions/addQuestion'\nimport generalInformationsServices from '../../../../api/generalInformationsServices'\nimport questionServices from '../../../../api/questionServices'\n\nexport default {\n  setup() {\n    return { v$: useVuelidate() };\n  },\n  name: 'Stage2',\n  created() {\n    this.getInformations();\n    this.getQuestions();\n  },\n  components: {\n    ListQuestion,\n  },\n  props: {\n        book: {\n            type: [Object],\n            required: true,\n        },\n\n    },\n  validations() {\n    return {\n      generalInfromationsform: {\n        general_question: {\n          required\n        },\n        summary: {\n          required\n        }\n      },\n      questionForm: {\n        question: {\n          required\n        },\n        pageStart: {\n          required,\n        },\n        pageEnd: {\n          required,\n        },      \n      },\n      quotes: {\n          required,\n        },\n\n    };\n  },\n  data() {\n    return {\n      questionForm: {\n        question: '',\n        pageStart: \"\",\n        pageEnd: \"\",\n      },\n      generalInfromationsform: {\n        general_question: '',\n        summary: '',\n        user_book_id:this.$route.params.id,\n      },\n      general_informations: [],\n      questions:[],\n      quotes: [],\n\n    }\n  },\n  methods: {\n    addField(value, fieldType) {\n      fieldType.push({ value: \"\" });\n    },\n    removeField(index, fieldType) {\n      fieldType.splice(index, 1);\n    },\n    async submitGenralInformationsForm() {\n      this.v$.$touch();\n      if (!this.v$.generalInfromationsform.$invalid) {\n        this.general_informations = await generalInformationsServices.addGeneralInformations(this.generalInfromationsform);\n\n      }\n    },\n    async submitQuestionForm() {\n      if (!this.v$.questionForm.$invalid) {\n        await questionServices.addQuestion(this.questionForm, this.quotes,this.$route.params.id);\n      }\n    },\n    async getInformations() {\n      this.general_informations = await generalInformationsServices.getGeneralInformations(this.$route.params.id);\n    },\n    async getQuestions() {\n      this.questions = await questionServices.getQuestions(this.$route.params.id);\n    }\n\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}